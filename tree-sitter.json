{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "midnite",
      "camelcase": "Midnite",
      "scope": "source.midnite",
      "path": ".",
      "file-types": ["mn"],
      "highlights": ["queries/highlights.scm"],
      "tags": ["queries/tags.scm"],
      "injection-regex": "^midnite$",
      "class-name": "TreeSitterMidnite"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Midnite grammar for tree-sitter",
    "authors": [
      {
        "name": "Hans Hoffman"
      }
    ],
    "links": {
      "repository": "https://github.com/rhoskal/midnite"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
